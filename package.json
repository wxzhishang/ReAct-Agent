    "example:custom": "bun run examples/custom-tool-example.ts",
    "example:siliconflow": "bun run examples/siliconflow-example.ts"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "consola": "^3.4.2",
    "mathjs": "^15.1.0",
    "openai": "^6.14.0",
    "zod": "^4.2.1"
  }
}
