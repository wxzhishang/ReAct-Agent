{
  "name": "react-agent",
  "version": "1.1.0",
  "description": "基于 ReAct 架构的 AI Agent 实现，支持自定义 API 端点",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run index.ts",
    "dev": "bun --watch index.ts",
